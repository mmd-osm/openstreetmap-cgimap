[Unit]
Description=CGImap implementation of the OpenStreetMap API
Documentation=https://wiki.openstreetmap.org/wiki/Cgimap
After=network.target

[Service]
Type=forking
ExecStart=/usr/local/bin/openstreetmap-cgimap --daemon
PIDFile=/var/run/cgimap.pid

[Install]
WantedBy=multi-user.target
Alias=cgimap
